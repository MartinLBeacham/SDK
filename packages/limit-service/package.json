  "main": "cjs/index.js",
  "module": "es/index.js",
  "exports": "./cjs/index.js",
  "source": "src/index.ts",
  "scripts": {
    "dev": "echo \"Implement me!\"",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' NODE_ENV=testing c8 --all --reporter text --reporter cobertura mocha --require ts-node/register './test/**/*.test.ts'",
    "lint": "eslint . --cache",
    "build": "rollup -c && tsc --declaration --emitDeclarationOnly --declarationDir ./types",
    "build:bundle": "esbuild src/index.ts --metafile=bundle/meta.json --bundle --format=cjs --outfile=bundle/index.js",
    "prepare": "NODE_ENV=production yarn build",
