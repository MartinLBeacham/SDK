  },
  "devDependencies": {
    "@date-fns/utc": "^1.1.0",
    "@rollup/plugin-typescript": "11.1.5",
    "@types/mocha": "^10.0.3",
    "@types/sinon": "^10.0.20",
    "c8": "8.0.1",
    "date-fns": "^2.30.0",
    "esbuild": "^0.19.5",
    "mocha": "10.2.0",
    "rollup": "2.79.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "should": "13.2.3",
    "sinon": "17.0.1",
